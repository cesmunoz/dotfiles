[user]
  name = Cesar Munoz
  email = Cesar Munoz
[includeIf "gitdir:~/repos/cm/"]
  path = "~/.gitconfigs/.cm"
[includeIf "gitdir:~/repos/cemdev/"]
  path = "~/.gitconfigs/.cm"
[includeIf "gitdir:~/repos/filestage/"]
  path = "~/.gitconfigs/.filestage"

[pull]
  rebase = true
[branch]
  autosetuprebase = always
[push]
  default = current
[core]
  autocrlf = false
  eol = lf
	excludesfile = /Users/cesmunoz/.gitignore_global
[init]
  defaultBranch = main
[alias]
  rh = reset --hard HEAD
  r = reset --hard
  ca = commit -a --verbose
  cm = commit -m 
  cp = cherry pick
  co = checkout
  b = checkout -b
  amend = commit -a --amend --no-edit
  unstage = reset --soft HEAD
  cdiff = "!git diff $1 $1 ^"
  br = branch
  s = status -s
  sw = switch
  swc = switch -c
  f = fetch 
  pu = push 
  pl = pull
  st = stash
  stsu = stash save -u
  stl = stash list
  la = "!git config -l | grep alias | cut -c 7-"
  l = log --pretty=oneline -n 20 --graph --abbrev-commit
  l1000 = log --pretty=oneline -n 1000 --graph --abbrev-commit
  branches = branch --all
  cred = config credential.helper 'cache --timeout=300'
  whoami = config user.email
  whoaminame = config user.name
  aliases = !git config --list | grep ^alias\\. | cut -c 7- | grep -Ei --color \"$1\" "#"
[color]
  ui = true
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
[color "diff"]
  meta = 11
  frag = magenta bold
  commit = yellow bold
  old = red bold
  new = green bold
  whitespace = red reverse
